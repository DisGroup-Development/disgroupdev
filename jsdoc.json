{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "opts": {
    "destination": "./docs/",
    "readme": "./README.md",
    "recurse": true,
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "dark",
      "meta": [
        {
          "name": "description",
          "content": "A powerful ðŸ”¥ NodeJS library for all data and managers of DisGroup Development."
        }
      ],
      "menu": [
        {
          "title": "Github",
          "id": "github",
          "link": "https://github.com/DisGroup-Development/disgroupdev.js",
          "target": "_blank"
        },
        {
          "title": "npm",
          "id": "npm",
          "link": "https://www.npmjs.com/package/disgroupdev.js",
          "target": "_blank"
        }
      ]
    }
  },
  "source": {
    "excludePattern": "(^|\\/|\\\\)_",
    "include": ["lib"]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
